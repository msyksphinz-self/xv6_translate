<redpen-conf lang="ja">
  <validators>
    <validator name="SentenceLength">
      <property name="max_len" value="100"/>
    </validator>
    <validator name="InvalidSymbol" />
    <validator name="KatakanaEndHyphen"/>
    <validator name="KatakanaSpellCheck"/>
    <validator name="SectionLength">
      <property name="max_num" value="1500"/>
    </validator>
    <validator name="ParagraphNumber"/>
    <validator name="DoubledWord">
	  <property name="list" value="、,で,を" />
	</validator>
    <validator name="SpaceBetweenAlphabeticalWord" />
    <validator name="CommaNumber" />
    <validator name="SuccessiveWord" />
    <validator name="JavaScript" />
  </validators>
  <symbols>
	<symbol name="LEFT_PARENTHESIS" value="(" invalid-chars="（" after-space="true" />
	<symbol name="RIGHT_PARENTHESIS" value=")" invalid-chars="）" after-space="true" />
  </symbols>
</redpen-conf>
